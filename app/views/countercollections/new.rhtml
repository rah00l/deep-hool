function checkoccerance1111(sText)

	{
   		
   		var Char;
		var count=0;
 
   		for (i = 0; i < sText.length; i++) 
      		{ 
      			Char = sText.charAt(i); 
      			if (Char=='-') 
      			{
      				count=count+1;
      			}
      			
      			if (count>1)
      			break;
      		}
      		
	  		return count;
   
   	}

function checkoccerance(sText)

{

		var Char;
		var count=0;
		if (sText.charAt(0)=='-' && sText.charAt(1)=='')
		{
			count=40;
		}
		else
		{
			for (i = 0; i < sText.length; i++)
			{
				Char = sText.charAt(i);
				if (Char=='-')
				{
					count=count+1;
					if(i>0)
					{
						count=50;
						break;
					}

				}

			}
		}
		return count;

}

