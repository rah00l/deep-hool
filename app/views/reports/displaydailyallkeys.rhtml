                                <span> - </span>
                              <%else%>
                                <span><%= avg_by_type %></span>
                              <% end %>
                      <B/></td>
                    <% end %>
                    </table>

      <br></br>

                            <table  border="1" width ="95%"  cellpadding="0"  cellspacing="0"  bordercolor="black"   id="AutoNumber1" >
