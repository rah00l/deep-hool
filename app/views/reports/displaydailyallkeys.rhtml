                                                    <%if @tsroutsecvalue.to_i>=0 %>
                                                      <td nowrap  height="10" align=center><font color=black><%= (((c.TMTROUT.to_i*c.MULTIPLY_BY*c.MTR_RATE_IN.to_i)/10).round) %></font></td>
