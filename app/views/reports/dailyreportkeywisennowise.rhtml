      <% end %>

      <% if(s.SHORTREASON=="XYZ") %>
      <% @totalXYZ=@totalXYZ.to_i+s.MTRSHORT.to_i%>
      <% @totalXYZ1=@totalXYZ1.to_i+s.MTRSHORT.to_i%>
