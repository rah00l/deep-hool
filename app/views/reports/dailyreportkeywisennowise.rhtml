      <% if(s.SHORTREASON=="REFUND") %>
      <% @totalREFUND=@totalREFUND.to_i+s.MTRSHORT.to_i%>
      <% @totalREFUND1=@totalREFUND1.to_i+s.MTRSHORT.to_i%>
