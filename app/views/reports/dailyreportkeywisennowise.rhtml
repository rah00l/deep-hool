<% @in1=0 %>
<% @out1=0 %>
<% @mpos1=0%>
<% @mneg1=0%>
<%for c in @c %>
	<% if(c.CALCULATEBY=='S')%>
		<% @in1=@in1.to_i+((c.TSRIN.to_i*c.SCREEN_RATE_IN.to_i*c.MULTIPLY_BY)/10).round%>
